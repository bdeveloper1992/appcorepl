	<table border=0 cellpadding=0 cellspacing=0 width='100%'>
		<tr>
			<tmpl_if nav_sibs>
				<td valign=top class='nav-sibs-cell'>
					<div class=nav-sibs>
						<ul>
							<tmpl_loop nav_sibs>
								<li>
									<a href='%%url%%' <tmpl_if current>class='current'</tmpl_if>>%%title%%</a>
								</li>
							</tmpl_loop>
						</ul>
					</div>
				</td>
			</tmpl_if>
			<td valign=top>
				<div class=pagebody>
					<!--<tmpl_if page_title>
						<h1>%%page_title%%</h1>
					</tmpl_if>-->
					
					%%page_content%%
				</div>
				
			</td>
		</tr>
	</table>