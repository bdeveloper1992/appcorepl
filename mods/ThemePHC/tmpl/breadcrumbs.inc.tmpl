		<tmpl_if nav_path>
			<div class='nav_level2'>
				<table cellspacing=0 cellpadding=0 border=0 width='100%'>
					<tr>
						<td width='100%'>
							<span class=breadcrumbs>
							<tmpl_if nav_path>
								<tmpl_loop nav_path>
									<tmpl_if current>
										<span class=current>%%title%%</span>
									<tmpl_else>
										<a href='%%url%%'>%%title%%</a><span class=divider> &raquo;</span>
									</tmpl_if>
								</tmpl_loop>
							</tmpl_if>
							</span>
							
							<tmpl_if nav_kids>
								<span class=divider>&raquo;</span>
								<span class=kids>(
								<tmpl_loop nav_kids>
								<a href='%%url%%'>%%title%%</a><tmpl_unless last><span class='or_divider'> | </span></tmpl_unless>
								</tmpl_loop>
								)</span>
							</tmpl_if>
						</td>
						<tmpl_if nav_url_from>
							<td nowrap>
								<div>
									<a href='%%nav_url_from%%'>&laquo; Return to the previous page</a>
								</div>
							</td>
						</tmpl_if>
						<td nowrap>
							<div class='no_float_wrap'>
								<span id='no_float_wrap'>
									<tmpl_if nav_url_from>
										&nbsp; | &nbsp;
									</tmpl_if>
									<a href='#' class='no_float'>Don't Float</a>
								</span>
							</div>
						</td>
					</tr>
				</table>
			</div>
		</tmpl_if>